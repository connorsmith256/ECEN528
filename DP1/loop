for benchmark in /ee2/ee628/Alpha-tools/spec2000benchmarks/*.eio.gz; do /ee2/ee628/Alpha-tools/simplesim-3.0/sim-outorder -max:inst $MAX_INSTR -cache:dl1 dl1:$L1SETS:$L1BSIZE:$L1ASSOC:$L1REPL -cache:il1 il1:$L1SETS:$L1BSIZE:$L1ASSOC:$L1REPL -cache:dl2 dl2:$L2SETS:$L2BSIZE:$L2ASSOC:$L2REPL -cache:il2 il2:$L2SETS:$L2BSIZE:$L2ASSOC:$L2REPL -cache:dl1lat $L1CACHE_LATENCY -cache:il1lat $L1CACHE_LATENCY -cache:dl2lat $L2CACHE_LATENCY -cache:il2lat $L2CACHE_LATENCY -mem:lat $MEM_LATENCY_FIRST $MEM_LATENCY_INNER $benchmark 2> $(basename $benchmark).out & done